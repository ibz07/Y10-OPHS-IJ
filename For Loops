# for loop =  a statement that will execute it's block of code
#             a limited amount of times
#
#             while loop = unlimited
#             for loop = limited

import time

#EXAMPLE 1:
for i in range(10):
   print(i + 1)

#EXAMPLE 2:
for i in range(50,100 + 1,2):
   print(i)

#EXAMPLE 2:
for i in "Ibrahim Jabbie":
    print(i)

#EXAMPLE 3:
for seconds in range(10,0,-1):
    print(seconds)
    time.sleep(2)
print("Happy New Year")
