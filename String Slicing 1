# slicing = create a substring by extracting elements from another sting
#           indexing[] or slice()
#           [start:stop:step]


name = "Ibrahim Jabbie"

first_name = name[0:]
last_name = name[4:]
funky_name = name[::6]
reversed_name = name[::-1]



print(first_name)
print(last_name)
print(funky_name)
print(reversed_name)
